generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  userId      String        @id @default(uuid())
  clerkId     String        @unique
  email       String        @unique
  firstName   String?
  lastName    String?
  avatarUrl   String?
  createdAt   DateTime      @default(now())
  updatedAt   DateTime      @updatedAt
}

